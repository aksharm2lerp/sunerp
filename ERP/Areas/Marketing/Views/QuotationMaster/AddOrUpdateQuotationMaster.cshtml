        if (inputDiscPer !== '' && inputDiscPer !== '0.00' && inputDiscPer !== '0' && inputDiscPer !== NaN) {
            const discount = parseFloat(Number(orgTotalAmt * document.getElementById('discountPerIndex' + inputValue).value)).toFixed(2);
            const discountAmt = parseFloat(Number(discount / 100)).toFixed(2);
            const afterDiscAmt = parseFloat(Number(orgTotalAmt - discountAmt)).toFixed(2);
            updatedAmt = afterDiscAmt;
            document.getElementById('discountAmtIndex' + inputValue).value = discountAmt;
        }
        document.getElementById('amtIndex' + inputValue).value = updatedAmt;
        fnUpdateNetAmount();
    }

    function fnUpdateDiscountPercentage(inputValue) {
        const orgDiscAmt = document.getElementById('rowOrgDiscAmt' + inputValue).value;

<script type="text/javascript">
    var urlsave = '@Url.Action("AddOrUpdateQuotationMaster", "QuotationMaster")';
    var orgTotalAmt = 0;
    function fnSubmit() {

        var sapStringJson = document.getElementById('SAPCollectionJSONString');

        var finalTable = document.getElementsByTagName('table');
        var rowLen = finalTable[0].rows.length;
        var netAmount = 0;
        var tableData = [];
        for (var i = 1; i < rowLen; i++) {

            let QuotationDetailId = finalTable[0].rows[i].cells[1].children[0].value;
            if (QuotationDetailId === '') {
                QuotationDetailId = 0;
            }
            let QuotationId = finalTable[0].rows[i].cells[2].children[0].value;
            if (QuotationId === '') {
                QuotationId = 0;
            }
            let itemId = finalTable[0].rows[i].cells[3].children[0].value;
            if (itemId === '') {
                itemId = 0;
            }
            var tester = finalTable[0].rows[i].cells[11].children[0].value;
            var rowData = {

                QuotationDetailID: QuotationDetailId,
                QuotationID: QuotationId,
                ItemID: itemId,
                UOMID: finalTable[0].rows[i].cells[4].children[0].value,
                ItemCode: finalTable[0].rows[i].cells[5].children[0].value,
                ItemName: finalTable[0].rows[i].cells[6].children[0].value,
                HSNcodes: finalTable[0].rows[i].cells[8].children[0].value,
                // UOM: finalTable[0].rows[i].cells[0].children[0].value,
                Qty: finalTable[0].rows[i].cells[10].children[0].value,
                Cancel_Qty: finalTable[0].rows[i].cells[11].children[0].value,
                Rate: finalTable[0].rows[i].cells[12].children[0].value,
                DiscountInPer: finalTable[0].rows[i].cells[13].children[0].value,
                DiscountAmount: finalTable[0].rows[i].cells[14].children[0].value,
                TotalAmount: finalTable[0].rows[i].cells[15].children[0].value,
                RequestForQuotID: finalTable[0].rows[i].cells[16].children[0].value,
                RequestForQuotDetailID: finalTable[0].rows[i].cells[17].children[0].value

            };
            tableData.push(rowData);
        }
        sapStringJson.value = JSON.stringify(tableData);

        if ($("#formQuotationMaster").valid()) {
            var l = laddaStart('btnSave');
            console.log(l);
            modeldata = $("#formQuotationMaster").serialize();
            $.ajax({
                type: 'POST',
                url: urlsave,
                data: modeldata,
                dataType: 'json',
                beforeSend: function () { },
                success: function (data) {
                    if (data.status) {
                        //notification
                        // $("#canvas_masterentity2").hide();//('hide');
                        Lobibox.notify('success', {
                            pauseDelayOnHover: true,
                            size: 'mini',
                            icon: 'bx bx-check-circle',
                            continueDelayOnInactiveTab: false,
                            position: 'bottom right',
                            msg: data.message
                        });
                        // window.location.href = '/Marketing/QuotationMaster/';
                    }
                    else {
                        Lobibox.notify('error', {
                            pauseDelayOnHover: true,
                            size: 'mini',
                            icon: 'bx bx-check-circle',
                            continueDelayOnInactiveTab: false,
                            position: 'bottom right',
                            msg: data.message
                        });
                    }
                },
                error: function (error) {
                    laddaStop(l);
                    Lobibox.notify('error', {
                        pauseDelayOnHover: true,
                        size: 'mini',
                        icon: 'bx bx-check-circle',
                        continueDelayOnInactiveTab: false,
                        position: 'bottom right',
                        msg: "@MessageHelper.Error"
                    });
                }
            }).always(function () {
                hideLoader();
            });
        }
    }


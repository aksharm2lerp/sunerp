    function removeRow(button) {

        var row = button.parentNode.parentNode.parentNode;

        let QuotationDetailId = row.cells[1].children[0].value;
        let QuotationId = row.cells[2].children[0].value;
        if (QuotationDetailId !== '' && QuotationDetailId !== NaN && QuotationId !== '' && QuotationId !== NaN) {
            var params = { QuotationDetailId: QuotationDetailId, QuotationId: QuotationId };
            $.ajax({
                type: 'POST',
                url: '/Marketing/QuotationMaster/QuotationDetailItemDeactivation',
                data: params,
                dataType: 'json',
                success: function (data) {
                    if (data.status) {
                        //notification
                        // $("#canvas_masterentity2").hide();//('hide');
                        Lobibox.notify('success', {
                            pauseDelayOnHover: true,
                            size: 'mini',
                            icon: 'bx bx-check-circle',
                            continueDelayOnInactiveTab: false,
                            position: 'bottom right',
                            msg: 'Item removed.'
                        });
                        //window.location.href = '/Marketing/QuotationMaster/';
                        row.parentNode.removeChild(row);
                    }
                    else {
                        Lobibox.notify('error', {
                            pauseDelayOnHover: true,
                            size: 'mini',
                            icon: 'bx bx-check-circle',
                            continueDelayOnInactiveTab: false,
                            position: 'bottom right',
                            msg: 'Something went wrong to remove item.'
                        });
                    }
                },
                error: function (error) {
                    laddaStop(l);
                    Lobibox.notify('error', {
                        pauseDelayOnHover: true,
                        size: 'mini',
                        icon: 'bx bx-check-circle',
                        continueDelayOnInactiveTab: false,
                        position: 'bottom right',
                        msg: "@MessageHelper.Error"
                    });
                }
            }).always(function () {
                hideLoader();
            });
        }
        else {
            row.parentNode.removeChild(row);
        }
    }


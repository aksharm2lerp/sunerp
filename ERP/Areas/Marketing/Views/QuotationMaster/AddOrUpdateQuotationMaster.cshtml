    function fnUpdateQuantity(inputValue) {
        const orgQty = parseFloat(document.getElementById('rowOrgQty' + inputValue).value || document.getElementById('qtyIndex' + inputValue).value);
        const orgCanclQty = document.getElementById('rowOrgCanclQty' + inputValue).value;
        const orgRate = document.getElementById('rowOrgRate' + inputValue).value;
        const orgDiscPer = document.getElementById('rowOrgDiscPer' + inputValue).value;
        const orgDiscAmt = document.getElementById('rowOrgDiscAmt' + inputValue).value;
        // const totalAmt;

        const inputCancelQty = document.getElementById('cancelQtyIndex' + inputValue).value;

        if (inputCancelQty !== '' && inputCancelQty !== '0.00' && inputCancelQty !== '0' && inputCancelQty !== NaN) {
            var inputQty = parseFloat(Number(orgQty - inputCancelQty)).toFixed(2);
            document.getElementById('qtyIndex' + inputValue).value = inputQty;
        }
        else {
            document.getElementById('qtyIndex' + inputValue).value = orgQty;
        }
        fnUpdateRate(inputValue);
        fnUpdateDiscount(inputValue);
        fnUpdateDiscountPercentage(inputValue);
    };

    function fnUpdateRate(inputValue) {
        const orgRate = document.getElementById('rowOrgRate' + inputValue).value;
        const inputRate = document.getElementById('rateIndex' + inputValue).value;
        const qty = document.getElementById('qtyIndex' + inputValue).value;

        if (inputRate !== '' && inputRate !== '0.00' && inputRate !== '0' && inputRate !== NaN) {
            document.getElementById('amtIndex' + inputValue).value = parseFloat(Number(qty * inputRate)).toFixed(2);
            document.getElementById('rowOrgTotalAmt' + inputValue).value = parseFloat(Number(qty * inputRate)).toFixed(2);
            if (parseFloat(document.getElementById('amtIndex' + inputValue).value) > 0) {
                fnUpdateNetAmount();
            }
        }
        else {
            document.getElementById('amtIndex' + inputValue).value = orgCanclQty;
        }
    }

    function fnUpdateDiscount(inputValue) {
        const inputDiscAmt = document.getElementById('discountAmtIndex' + inputValue).value;
        const inputDiscPer = document.getElementById('discountPerIndex' + inputValue).value;
        const rateAmt = document.getElementById('amtIndex' + inputValue).value;
        const orgTotalAmt = document.getElementById('rowOrgTotalAmt' + inputValue).value;
        var updatedAmt = 0;
        // if (inputDiscAmt !== '' && inputDiscAmt !== '0.00' && inputDiscAmt !== '0' && inputDiscAmt !== NaN) {
        //     let discount = Number(document.getElementById('discountAmtIndex' + inputValue).value * 100);

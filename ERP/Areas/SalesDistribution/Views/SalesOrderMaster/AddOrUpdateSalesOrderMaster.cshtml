    function removeRow(button) {

        var row = button.parentNode.parentNode.parentNode;

        let salesOrderDetailId = row.cells[1].children[0].value;
        let salesOrderId = row.cells[2].children[0].value;
        if (salesOrderDetailId !== '' && salesOrderId !== '' && salesOrderDetailId !== NaN && salesOrderId !== NaN && salesOrderId !== '0' && salesOrderDetailId !== '0') {
            var params = { salesOrderDetailId: salesOrderDetailId, salesOrderId: salesOrderId };
            $.ajax({
                type: 'POST',
                url: '/SalesDistribution/SalesOrderMaster/SalesOrderDetailItemDeactivation',
                data: params,
                dataType: 'json',
                success: function (data) {
                    if (data.status) {
                        //notification
                        // $("#canvas_masterentity2").hide();//('hide');
                        Lobibox.notify('success', {
                            pauseDelayOnHover: true,
                            size: 'mini',
                            icon: 'bx bx-check-circle',
                            continueDelayOnInactiveTab: false,
                            position: 'bottom right',
                            msg: 'Item removed.'
                        });
                        //window.location.href = '/SalesDistribution/SalesOrderMaster/';
                        row.parentNode.removeChild(row);
                    }
                    else {
                        Lobibox.notify('error', {
                            pauseDelayOnHover: true,
                            size: 'mini',
                            icon: 'bx bx-check-circle',
                            continueDelayOnInactiveTab: false,
                            position: 'bottom right',
                            msg: 'Something went wrong to remove item.'
                        });
                    }
                },
                error: function (error) {
                    laddaStop(l);
                    Lobibox.notify('error', {
                        pauseDelayOnHover: true,
                        size: 'mini',
                        icon: 'bx bx-check-circle',
                        continueDelayOnInactiveTab: false,
                        position: 'bottom right',
                        msg: "@MessageHelper.Error"
                    });
                }
            }).always(function () {
                hideLoader();
            });
        }
        else{
            row.parentNode.removeChild(row);
        }
    }


<script type="text/javascript">
    var urlsave = '@Url.Action("AddOrUpdateSalesOrderMaster", "SalesOrderMaster")';
    function fnSubmit() {

        document.getElementById('NetAmountDestination').value = document.getElementById('NetAmountSource').value;
        document.getElementById('FreightPercentageDestination').value = document.getElementById('FreightPercentageSource').value;
        document.getElementById('FreightAmountDestination').value = document.getElementById('FreightAmountSource').value;
        document.getElementById('PackingChargesPerDestination').value = document.getElementById('PackingChargesPerSource').value;
        document.getElementById('PackingAmountDestination').value = document.getElementById('PackingAmountSource').value;
        document.getElementById('GrossAmountDestination').value = document.getElementById('GrossAmountSource').value;

        var sapStringJson = document.getElementById('SAPCollectionJSONString');

        var finalTable = document.getElementsByTagName('table');
        var rowLen = finalTable[0].rows.length;

        var tableData = [];
        for (var i = 1; i < rowLen; i++) {

            let salesOrderDetailId = finalTable[0].rows[i].cells[1].children[0].value;
            if (salesOrderDetailId === '') {
                salesOrderDetailId = 0;
            }
            let salesOrderId = finalTable[0].rows[i].cells[2].children[0].value;
            if (salesOrderId === '') {
                salesOrderId = 0;
            }
            let itemId = finalTable[0].rows[i].cells[3].children[0].value;
            if (itemId === '') {
                itemId = 0;
            }
            var rowData = {

                // SalesOrderDetailID: salesOrderDetailId,
                // SalesOrderID: salesOrderId,
                // ItemID: itemId,
                // UOMID: finalTable[0].rows[i].cells[4].children[0].value,
                // ItemCode: finalTable[0].rows[i].cells[5].children[0].value,
                // ItemName: finalTable[0].rows[i].cells[6].children[0].value,
                // HSNcodes: finalTable[0].rows[i].cells[8].children[0].value,
                // // UOM: finalTable[0].rows[i].cells[0].children[0].value,
                // Qty: finalTable[0].rows[i].cells[10].children[0].value,
                // Rate: finalTable[0].rows[i].cells[11].children[0].value,
                // TotalAmount: finalTable[0].rows[i].cells[12].children[0].value

                SalesOrderDetailID: salesOrderDetailId,
                SalesOrderID: salesOrderId,
                ItemID: itemId,
                UOMID: finalTable[0].rows[i].cells[4].children[0].value,
                ItemCode: finalTable[0].rows[i].cells[5].children[0].value,
                ItemName: finalTable[0].rows[i].cells[6].children[0].value,
                UOM: finalTable[0].rows[i].cells[7].children[0].value,
                StockNoOfCoil: finalTable[0].rows[i].cells[9].children[0].value,
                StockCoilLength: finalTable[0].rows[i].cells[10].children[0].value,
                OrderNoOfCoil: finalTable[0].rows[i].cells[11].children[0].value,
                OrderCoilLength: finalTable[0].rows[i].cells[12].children[0].value,
                OrderQtyInMeter: finalTable[0].rows[i].cells[13].children[0].value,
                PriceListAmt: finalTable[0].rows[i].cells[14].children[0].value,
                PerMeterPrice: finalTable[0].rows[i].cells[15].children[0].value,
                DiscountAmount: finalTable[0].rows[i].cells[16].children[0].value,
                SpecialDiscount: finalTable[0].rows[i].cells[17].children[0].value,
                CashDiscount: finalTable[0].rows[i].cells[18].children[0].value,
                TotalAmount: finalTable[0].rows[i].cells[19].children[0].value,
            };

            tableData.push(rowData);
        }
        sapStringJson.value = JSON.stringify(tableData);

        if ($("#formSalesOrderMaster").valid()) {
            var l = laddaStart('btnSave');
